isbn = {978-0-8243-3717-9},
issn = {1527-8204},
journal = {Annual review of genomics and human genetics},
keywords = {CTCF,TAD,chromosome conformation capture,cohesin,ctcf,gene regulation,genome folding,loop extrusion,tad},
