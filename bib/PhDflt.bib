file = {:home/ibnsalem/Mendeley/Creyghton et al. - 2010 - Histone H3K27ac separates active from poised enhancers and predicts developmental state.pdf:pdf},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
month = {dec},
