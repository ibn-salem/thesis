@article{Smemo2014,
abstract = {The search for genetic correlates of obesity has highlighted a noncoding region in the FTO gene: variations within this intron are associated with increased risk for obesity and type 2 diabetes. Although the biological actions of FTO have been intensely scrutinized, it is still not clear how these genetic variants influence FTO expression and biology. This paper shows that these noncoding sequences are functionally connected at megabase distances with the homeobox gene IRX3. The obesity-associated interval appears to belong to the regulatory functions of IRX3, rather than FTO. In addition, mice lacking Irx3 have reduced body weight and are resistant to diet-induced obesity. Taken together, the data suggest that IRX3 is an important metabolic regulator associated with human obesity and type 2 diabetes.},
