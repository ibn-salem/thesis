file = {:home/ibnsalem/Mendeley/Joshi et al. - 2015 - Dynamic Reorganization of Extremely Long-Range Promoter-Promoter Interactions between Two States of Pluripotency.pdf:pdf},
issn = {1934-5909},
journal = {Cell Stem Cell},
month = {dec},
