doi = {10.1101/gr.161620.113},
file = {:home/ibnsalem/Mendeley/Seitan et al. - 2013 - Cohesin-Based chromatin interactions enable regulated gene expression within preexisting architectural compart(2).pdf:pdf},
isbn = {1549-5469 (Electronic) 1088-9051 (Linking)},
issn = {10889051},
journal = {Genome Research},
