isbn = {13624962 (Electronic)},
issn = {1362-4962},
journal = {Nucleic acids research},
number = {November 2015},
pages = {gkv1176},
pmid = {26531826},
title = {{JASPAR 2016: a major expansion and update of the open-access database of transcription factor binding profiles}},
