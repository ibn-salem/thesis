pages = {1391--400},
pmid = {26163319},
title = {{Saturation analysis of ChIP-seq data for reproducible identification of binding peaks.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26163319 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4561497},
