journal = {Bioinformatics (Oxford, England)},
keywords = {Binding Sites,Chromatin Immunoprecipitation,Computer Simulation,DNA,Deoxyribonuclease I,Genomics,Genomics: methods,Sequence Analysis,Software,Transcription Factors,Transcription Factors: metabolism},
month = {aug},
number = {16},
pages = {2046--8},
pmid = {23782611},
title = {{GAT: a simulation framework for testing the association of genomic intervals.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3722528{\&}tool=pmcentrez{\&}rendertype=abstract},
