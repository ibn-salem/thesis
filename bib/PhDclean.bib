journal = {BMC evolutionary biology},
keywords = {Animals,Chickens,Chickens: genetics,Chromosome Breakage,Dogs,Evolution,Genetic Markers,Genetic Markers: genetics,Genomic Instability,Genomic Instability: genetics,Genomics,Genomics: methods,Humans,Mice,Pan troglodytes,Pan troglodytes: genetics,Rats},
