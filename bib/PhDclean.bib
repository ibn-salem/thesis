keywords = {Animals,Chromosomes,Cytogenetics,Drosophila melanogaster,Drosophila melanogaster: genetics,Gene Expression,Gene Expression: genetics,Genes,Heterochromatin,Heterochromatin: genetics,Insect,Insect: genetics,Telomere},
pages = {577--605},
pmid = {8825487},
title = {{Heterochromatin and gene expression in Drosophila.}},
