journal = {Annual Review of Genomics and Human Genetics},
keywords = {Animals,Comparative Genomic Hybridization,DNA Re,Disease,Evolution,Gene Dosage,Humans,Molecular,Polymorphism,Single Nucleotide,chemistry/genetics,genetics},
month = {sep},
