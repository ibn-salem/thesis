journal = {Nucleic acids research},
keywords = {Animals,Computational Biology,Databases,Embryo,Enhancer Elements,Gene Expression,Genetic,Genome,Genomics,Human,Humans,Internet,Mammalian,Mammalian: metabolism,Mice,Nucleic Acid,User-Computer Interface},
