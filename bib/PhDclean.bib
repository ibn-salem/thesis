@article{Cabili2011,
abstract = {Large intergenic noncoding RNAs (lincRNAs) are emerging as key regulators of diverse cellular processes. Determining the function of individual lincRNAs remains a challenge. Recent advances in RNA sequencing (RNA-seq) and computational methods allow for an unprecedented analysis of such transcripts. Here, we present an integrative approach to define a reference catalog of {\textgreater}8000 human lincRNAs. Our catalog unifies previously existing annotation sources with transcripts we assembled from RNA-seq data collected from ∼4 billion RNA-seq reads across 24 tissues and cell types. We characterize each lincRNA by a panorama of {\textgreater}30 properties, including sequence, structural, transcriptional, and orthology features. We found that lincRNA expression is strikingly tissue-specific compared with coding genes, and that lincRNAs are typically coexpressed with their neighboring genes, albeit to an extent similar to that of pairs of neighboring protein-coding genes. We distinguish an additional subset of transcripts that have high evolutionary conservation but may include short ORFs and may serve as either lincRNAs or small peptides. Our integrated, comprehensive, yet conservative reference catalog of human lincRNAs reveals the global properties of lincRNAs and will facilitate experimental studies and further functional classification of these genes.},
author = {Cabili, Moran N and Trapnell, Cole and Goff, Loyal and Koziol, Magdalena and Tazon-Vega, Barbara and Regev, Aviv and Rinn, John L},
doi = {10.1101/gad.17446611},
file = {:home/ibnsalem/Mendeley/Cabili et al. - 2011 - Integrative annotation of human large intergenic noncoding RNAs reveals global properties and specific subclas(2).pdf:pdf},
issn = {1549-5477},
journal = {Genes {\&} development},
month = {sep},
number = {18},
pages = {1915--27},
pmid = {21890647},
publisher = {Cold Spring Harbor Laboratory Press},
title = {{Integrative annotation of human large intergenic noncoding RNAs reveals global properties and specific subclasses.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21890647 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3185964},
volume = {25},
year = {2011}
}
