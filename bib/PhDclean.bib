issn = {2160-1836},
journal = {G3 (Bethesda, Md.)},
keywords = {FISH,ISCN 2013,chromosomal microarray analysis,karyotype},
month = {jul},
number = {7},
pages = {1143--9},
pmid = {23665875},
publisher = {G3: Genes, Genomes, Genetics},
title = {{A dynamic database of microarray-characterized cell lines with various cytogenetic and genomic backgrounds.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23665875 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3704242},
